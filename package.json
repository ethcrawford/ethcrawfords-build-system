  "private": false,
  "scripts": {
    "start": "brunch watch --server",
    "watch": "brunch watch",
    "build-dev": "brunch build",
    "build": "brunch build --production",
    "static": "static public",
    "sprite": "coffee sprite.coffee",
    "clean-public": "rm -rf public/",
    "clean-sprite": "rm -rf sprite/",
    "stylus-repl": "stylus --interactive",
    "html2pug": "html2pug < transpil/html/source.html > transpil/html/source.pug",
    "pug2html": "pug transpil/pug/source.pug -w -P",
    "css2stylus": "stylus -C transpil/css/source.css",
    "stylus2css": "stylus transpil/stylus/source.styl -w",
    "js2coffee": "js2coffee transpil/js/source.js | tee -a transpil/js/source.coffee",
    "coffee2js": "coffee -w -c transpil/coffee/source.coffee"
  },
  "devDependencies": {
    "auto-reload-brunch": "^2.7.1",
    "autoprefixer": "^8.4.1",
